generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model EmailLog {
  id        Int      @id @default(autoincrement())
  email     String
  timestamp DateTime @default(now())
    status    String   @default("Delivered") // âœ… this line is required
}
