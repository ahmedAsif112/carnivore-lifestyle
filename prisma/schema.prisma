generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model EmailLog {
  id        Int      @id @default(autoincrement())
  email     String
  timestamp DateTime @default(now())
    status    String   @default("Delivered") // âœ… this line is required
}
