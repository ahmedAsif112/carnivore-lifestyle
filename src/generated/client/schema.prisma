generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model EmailLog {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  timestamp DateTime @default(now())
  status    String   @default("Delivered")
}
